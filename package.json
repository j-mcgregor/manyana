{
  "name": "manyana-labs",
  "version": "1.0.0",
  "description": "Portfolio site for ManyanaDev",
  "repository": "https://github.com/j-mcgregor/manyana",
  "author": "jmcgregor <jackjwmcgregor@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:inspect": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --ext ts --ext tsx --ext js --fix && next lint",
    "pretty": "prettier --write **/*.tsx **/*.ts",
    "test": "jest --silent --runInBand --watchAll=false",
    "test:watch": "jest --runInBand --watchAll --forceExit --detectOpenHandles",
    "test:coverage": "yarn jest --coverage ",
    "clean": "rm -rf node_modules coverage .next yarn.lock",
    "cz": "git-cz",
    "prepare": "husky install && next telemetry disable"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.2",
    "classnames": "^2.3.2",
    "cookies-next": "^2.1.1",
    "next": "12.1.6",
    "next-auth": "^4.3.1",
    "next-intl": "^2.7.5",
    "node-fetch": "^3.2.10",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-hook-form": "^7.29.0",
    "react-icons": "^4.4.0",
    "react-parallax": "^3.5.1",
    "react-scroll": "^1.8.8",
    "react-timeago": "^6.2.1",
    "recoil": "^0.7.0",
    "semantic-release": "^19.0.5",
    "swiper": "^8.4.2",
    "swr": "^1.2.2"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/typography": "^0.5.2",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^13.0.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^14.0.4",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "@types/react-scroll": "^1.8.4",
    "@types/react-timeago": "^4.1.3",
    "@types/tailwindcss": "^3.0.10",
    "autoprefixer": "^10.4.4",
    "eslint": "8.14.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.1.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sonarjs": "^0.13.0",
    "git-cz": "^4.8.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "postcss": "^8.4.12",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "tailwindcss": "^3.0.23",
    "typescript": "<4.6.0"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/changelog",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      "@semantic-release/git",
      "@semantic-release/github",
      "@semantic-release/release-notes-generator"
    ]
  }
}
